pipeline
{
  stages
    {
      stage('One') {
          steps {
              echo "This is a basic pipeline test---stage1---step1"
            }
          steps {
              echo "This is a basic pipeline test----stage1--step2"
            }
        }
      stage('Two') {
          steps {
              echo "This is a basic pipeline test---stage2---step1"
            }
          steps {
              echo "This is a basic pipeline test----stage2--step2"
            }
        }
    }
}       
